
// interface
interface in1{
    //variable by default public static final
    int a=10;
    //abstract method by default public
    void display();
}

//class
class testclass1 implements in1
{
    public void display()
    {
        System.out.println("this is test class 1");
    }
    public static void main(String[] arg)
    {
        testclass1 t1=new testclass1();
        t1.display();
        System.out.println(t1.a);
    }
}
