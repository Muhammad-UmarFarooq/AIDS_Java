class Employee{
    int salary = 60000;
}

class Engineer extends Employee{
    int benefits = 10000;
}
// Driver class
class main{
    public static void main(String args[])
    {
        Engineer e1= new Engineer();
        System.out.println("Salary :"+e1.salary+"\nBenefits : "+e1.benefits);
    }
}
/*
Output:

Salary :60000
Benefits : 10000
*/
